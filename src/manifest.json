{
  "manifest_version": 2,
  "name": "Yandex Search Engine",
  "short_name": "yandex_search_engine",
  "description": "Adds Yandex search engine to Firefox.",
  "version": "1.0.1",
  "icons": {
    "16": "icons/yandex.svg",
    "64": "icons/yandex.svg",
    "256": "icons/yandex.svg"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Yandex search engine",
      "keyword": "@yandex",
      "favicon_url": "https://upload.wikimedia.org/wikipedia/commons/thumb/5/58/Yandex_icon.svg/24px-Yandex_icon.svg.png",
      "search_url": "https://yandex.com/search/?text={searchQuery}"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "YandexSearchEngineExtension@space.kdev",
      "strict_min_version": "55.0"
    }
  },
  "author": "KDev",
  "homepage_url": "https://github.com/kdevspace/firefox-yandex-search-ext"
}
